{
  "name": "react-joyride-demo",
  "version": "0.5.0",
  "description": "Create walkthoughs and guided tours for your apps",
  "author": "Gil Barbara <gilbarbara@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gilbarbara/react-joyride.git"
  },
  "homepage": "https://gilbarbara.github.com/react-joyride",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "classlist-polyfill": "^1.0",
    "core-decorators": "^0.12",
    "core-js": "^2.4",
    "react": "^15.3",
    "react-dom": "^15.3",
    "react-joyride": "^1.4"
  },
  "devDependencies": {
    "babel-eslint": "^6.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13",
    "babel-preset-react": "^6.11",
    "babel-preset-stage-0": "^6.5",
    "babelify": "^7.3",
    "browser-sync": "^2.14",
    "browserify": "^13.1",
    "connect": "^3.4",
    "connect-history-api-fallback": "^1.3",
    "del": "^2.2",
    "eslint": "3.3",
    "eslint-config-airbnb": "^10.0",
    "eslint-plugin-react": "^6.1",
    "gulp": "^3.9",
    "gulp-autoprefixer": "^3.1",
    "gulp-cssmin": "^0.1",
    "gulp-eslint": "^3.0",
    "gulp-flatten": "^0.3",
    "gulp-gh-pages": "^0.5",
    "gulp-if": "^2.0",
    "gulp-imagemin": "^3.0",
    "gulp-load-plugins": "^1.2",
    "gulp-plumber": "^1.1",
    "gulp-rename": "^1.2",
    "gulp-sass": "^2.3",
    "gulp-size": "^2.1",
    "gulp-tap": "^0.1",
    "gulp-uglify": "^2.0",
    "gulp-useref": "^3.1",
    "gulp-util": "^3.0",
    "livereactload": "^3.0",
    "merge-stream": "^1.0",
    "react-proxy": "^1.1",
    "run-sequence": "^1.2",
    "vinyl-buffer": "^1.0",
    "vinyl-paths": "^2.1",
    "vinyl-source-stream": "^1.1",
    "watchify": "^3.7"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "livereactload/babel-transform",
                  "imports": [
                    "react"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": "/bower_components/",
          "sourceMapRelative": "."
        }
      ]
    ]
  },
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  }
}
