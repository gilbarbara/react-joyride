    "scroll-doc": "^0.2.1",
    "scrollparent": "^2.0.1",
    "tree-changes": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
